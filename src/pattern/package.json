{
  "name": "@sigyn/pattern",
  "version": "1.1.0",
  "description": "Loki pattern in JavaScript",
  "main": "./dist/pattern.cjs",
  "module": "./dist/pattern.js",
  "types": "./dist/pattern.d.ts",
  "exports": {
    ".": {
      "types": "./dist/pattern.d.ts",
      "require": "./dist/pattern.cjs",
      "import": "./dist/pattern.js"
    }
  },
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsup src/pattern.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test-types": "npm run build && tsd",
    "test-only": "node --test ./test/**/*.spec.ts",
    "test": "npm run test-only && npm run test-types",
    "coverage": "c8 -r html npm test",
    "lint": "eslint src test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAlly/sigyn",
    "directory": "src/pattern"
  },
  "bugs": {
    "url": "https://github.com/OpenAlly/sigyn/issues"
  },
  "homepage": "https://github.com/OpenAlly/sigyn/blob/main/src/pattern/README.md",
  "files": [
    "dist"
  ],
  "keywords": [
    "grafana",
    "loki",
    "pattern",
    "log",
    "logs"
  ],
  "tsd": {
    "directory": "/test/types"
  },
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "tsd": "^0.33.0"
  },
  "type": "module"
}
