{
  "name": "@sigyn/logql",
  "version": "2.2.0",
  "description": "Parse and serialize LogQL queries",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "glob -c \"tsx --test\" \"./test/**/*.spec.ts\"",
    "coverage": "c8 -r html npm test",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAlly/sigyn.git",
    "directory": "src/logql"
  },
  "bugs": {
    "url": "https://github.com/OpenAlly/sigyn/issues"
  },
  "homepage": "https://github.com/OpenAlly/sigyn/blob/main/src/logql/README.md",
  "files": [
    "dist"
  ],
  "keywords": [],
