{
  "name": "@sigyn/config",
  "version": "4.0.0",
  "description": "Sigyn configuration manager",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "node --test ./test/**/*.spec.ts",
    "coverage": "c8 -r html npm test",
    "lint": "eslint src test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAlly/sigyn",
    "directory": "src/config"
  },
  "bugs": {
    "url": "https://github.com/OpenAlly/sigyn/issues"
  },
  "homepage": "https://github.com/OpenAlly/sigyn/blob/main/src/config/README.md",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@myunisoft/loki": "^6.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "minimatch": "^10.0.1"
  },
  "devDependencies": {
    "@myunisoft/httpie": "^5.0.1"
  }
}
